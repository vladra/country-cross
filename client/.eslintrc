{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  rules: {
    "import/no-unresolved": [1, {
      ignore: [
        "^components",
        "^store",
        "^utils",
        "^gql"
      ]
    }],
    "import/extensions": ["error", "never"],
    "import/no-extraneous-dependencies": [0]
  }
}
